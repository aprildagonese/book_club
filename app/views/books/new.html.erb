<%= form_for @book do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>

  <%= f.label :length %>
  <%= f.number_field :length %>

  <%= f.label :year %>
  <%= f.number_field :year %>

  <%= f.label :cover_image%>
  <%= f.text_field :cover_image %>

  <%= f.label :author %>
  <%= f.text_field :author %>

  <%= f.submit %>
<% end %>
